    "@kubb/swagger-ts": "2.28.4",
    "@kubb/swagger-zod": "2.28.4",
    "@repo/typescript-config": "workspace:*",
    "tsup": "^8.5.1",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "zod": "^3.22.0"
  },
